<Application x:Class="Win10NightLightThemeSync.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Win10NightLightThemeSync"
             ShutdownMode="OnExplicitShutdown"
             >
    <Application.Resources>
        <BitmapImage x:Key="DarkIcon" UriSource="pack://application:,,,/Assets/icon_dark.ico" DecodePixelHeight="48" DecodePixelWidth="48" CacheOption="OnDemand"/>
        <BitmapImage x:Key="LightIcon" UriSource="pack://application:,,,/Assets/icon_light.ico" DecodePixelHeight="48" DecodePixelWidth="48" CacheOption="OnDemand"/>
    </Application.Resources>
</Application>
